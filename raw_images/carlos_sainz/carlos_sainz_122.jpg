--typography-presets-utility-subheading010-letter-spacing: 0;
--typography-presets-utility-subheading010-line-height: 1.125;;
  }
  /* dynamic vars (index 1 rule) */
  :host {
    --items-visibility: hidden;
    --items-row-height: minmax(0, 0fr);
    --icon-rotation: 0deg;
  }
  /* static mode vars (light as default if no mode attr provided) */
  :host(:not([mode='dark'])) {
